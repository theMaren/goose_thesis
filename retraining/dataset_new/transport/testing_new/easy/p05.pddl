    (road l6 l1)
    (road l5 l1)
    (road l4 l2)
    (road l2 l3)
    (road l1 l6)
    (road l3 l2)
    (road l2 l5)
    (road l5 l2)
    (road l4 l5)
    (road l5 l4)
    (road l1 l3)
    (road l3 l1)
    (road l2 l6)
    (road l6 l2)
    (road l1 l2)
    (road l2 l1)
    (road l3 l6)
    (road l6 l3)
    (road l1 l4)
    (road l4 l1)
